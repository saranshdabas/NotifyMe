{
  "name": "notifyme",
  "version": "1.0.0",
  "description": "A vaccine notifier to get latest results for the available slots in India",
  "main": "index.js",
  "scripts": {
    "start": "npm run prod",
    "build": "npm run clean && npm run transpile",
    "server": "node ./dist-server/index.js",
    "dev": "NODE_ENV=development npm run build && npm run server",
    "prod": "NODE_ENV=production npm run build && npm run server",
    "transpile": "babel ./src --out-dir dist-server",
    "clean": "rm -rf dist-server",
    "watch:dev": "nodemon"
  },
  "author": "Saransh",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "googleapis": "^73.0.0",
    "mongoose": "^5.12.7",
    "mongoose-unique-validator": "^2.0.3",
    "node-schedule": "^2.0.0",
    "nodemailer": "^6.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/runtime": "^7.14.0",
    "babel-loader": "^8.2.2",
    "nodemon": "^2.0.7"
  }
}
